import requests
from bs4 import BeautifulSoup

INDEED_URL = "http://yundata.cafe24.com/bbs/board.php?bo_table=sub01_03&page=139"
indeed_result = requests.get(
    "http://yundata.cafe24.com/bbs/board.php?bo_table=sub01_03&page=139")

indeed_soup = BeautifulSoup(indeed_result.text, "html.parser")

pagination = indeed_soup.find("span", {"class": "sound_only"})

links = pagination.find_all('a')
pages = []


for link in links:
    spans.append(link.string)
pages = pages[0:-1]
print(pages)

